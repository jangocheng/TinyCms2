@model IEnumerable<PostOverviewModel>
@if (Model.Any())
{
    <div class="other_news">
        <h2>Tin tức khác</h2>
        <ul>
            @foreach (var post in Model)
            {
                <li>
                    <a href="@Url.RouteUrl("Post", new {SeName = post.SeName})">
                        <span class="loading"><img src="@post.DefaultPictureModel.ImageUrl" alt="@post.DefaultPictureModel.AlternateText" /></span><h2>@post.Name</h2>
                    </a>
                </li>
            }
        </ul>
    </div>
}

