@model IList<PostOverviewModel>

@if (Model.Count > 0)
{
    <div class="related_news">
        <h2>Tin liên quan</h2>
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="@Url.RouteUrl("Post", new {item.SeName})">@item.Name</a></li>
            }
        </ul>
    </div>
}
